{
 "compilerOptions": {
  "target": "ES6",
  "module": "NodeNext",
  // "module": "ESNext",
  "moduleResolution": "NodeNext",
  // "moduleResolution": "node",
  "lib": ["ES6"],
  "sourceMap": true,
  "strict": true,
  "noImplicitAny": true,
  "esModuleInterop": true,
  "resolveJsonModule": true,
  "allowJs": true,
  "rootDir": "./src",
  "outDir": "./dist"
 },
 "include": ["src/**/*"]
}